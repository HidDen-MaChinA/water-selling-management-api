{
  "name": "water-selling-management",
  "version": "1.0.0",
  "description": "backend to manage a queu of people bying water and save there informations for analytics",
  "license": "ISC",
  "author": "niainafrancoroberto@gmail.com",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "dev": "rollup  --config rollup.config.ts --configPlugin typescript --watch",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "start": "node ./dist/server.js --watch",
    "gen/analytics/data": "ts-node ./src/scripts/generateAnalyticsData.ts",
    "del/duplicate/analytics/data": "ts-node ./src/scripts/deleteDuplicateAnalyticsData.ts",
    "dev/prisma/migrate": "prisma migrate dev",
    "dev/prisma/generate": "prisma generate",
    "build/workers": "rollup --config rollup.workers.config.ts --configPlugin typescript",
    "dev/workers": "rollup --config rollup.workers.config.ts --configPlugin typescript --watch",
    "start/workers": "node ./dist/jobs/index.js"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@rollup/plugin-commonjs": "^29.0.0",
