    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.3.0",
    "blob": "^0.1.0",
    "glob": "^13.0.6",
    "nodemon": "^3.1.11",
    "rollup-plugin-livereload": "^2.0.5",
