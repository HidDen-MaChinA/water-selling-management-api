    "@rollup/plugin-typescript": "^12.3.0",
    "axe": "^13.0.0",
    "bcryptjs": "^3.0.3",
    "bree": "^9.2.9",
    "cabin": "^14.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "rollup": "^4.57.1",
    "signale": "^1.4.0",
    "tslib": "^2.8.1",
    "yup": "^1.7.1"
  },
